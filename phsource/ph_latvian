phoneme a
  vowel starttype #a endtype #a
  length 120
  ChangeIfStressed(a+)
  ChangeIfDiminished(a-)
  ChangeIfNotStressed(a-)
  FMT(vowel/aa_7, 70) // possible variants: a_3, a_5 or aa_7
endphoneme

phoneme a+
  vowel starttype #a endtype #a
  length 120
  FMT(vowel/aa_7, 100)
endphoneme

phoneme a-
  vowel starttype #a endtype #a
  length 120
  FMT(vowel/aa_7, 60)
endphoneme

phoneme a:
  vowel starttype #a endtype #a
  length 300
  ChangeIfStressed(a:+)
  ChangeIfDiminished(a:-)
  long
  FMT(vowel/aa_9, 60) // was a_3 or aa_9
endphoneme

phoneme a:+
  vowel starttype #a endtype #a
  length 300
  long
  FMT(vowel/aa_9, 100)
endphoneme

phoneme a:-
  vowel starttype #a endtype #a
  length 300
  long
  FMT(vowel/aa_9, 60)
endphoneme

