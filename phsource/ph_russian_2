phoneme O
  vowel starttype (o) endtype (o)
  length 100
  formants vwl_ru/O
//  reduceto V  4
endphoneme

phoneme o
  vowel starttype (o) endtype (o)
  length 100
  formants vwl_ru/o
//  reduceto V  4
endphoneme

phoneme u
  vowel starttype (u) endtype (u)
  length 150
  formants vwl_ru/u
endphoneme


phoneme u#          
  vowel starttype (u) endtype (u)
  length 80
  formants vwl_ru/u#
endphoneme

phoneme u"        
  vowel starttype (u) endtype (u)
  length 150
  formants vwl_ru/u"
endphoneme

phoneme yo          
  vowel starttype (o) endtype (o)
  length 150
  formants vwl_ru/yo
  reduceto V  4
endphoneme


// softened vowels

phoneme e
  vowel starttype (e) endtype (e)
  length 190
  formants vowel/e
//  reduceto I  4
endphoneme


