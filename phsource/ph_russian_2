phoneme ju
  vowel starttype (u) endtype (u)
  length 190
  formants vwl_ru/ju
endphoneme


// reduced vowels

phoneme I
  vowel  starttype (i) endtype (a)
  length 200
  formants vwl_ru/I
 // linkout ;
endphoneme


phoneme V
  vowel  starttype (a)  endtype (@)
  length 100
  formants vowel/V_2
endphoneme


phoneme U
  vowel  starttype (u)  endtype (u)
  length 140
  formants vowel/uu
endphoneme


phoneme Y
  vowel  starttype (u)  endtype (u)
  length 140
  formants vowel/ii#
endphoneme


phoneme a
  vowel   starttype (a)  endtype (a)
  formants vwl_ru/a
  length 100
endphoneme


