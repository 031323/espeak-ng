

//=========================================================
// Dutch
//=========================================================


// Vowels
//=======


phoneme @   //  Schwa
  vowel starttype (@) endtype (@)
  formants vowel/@
  unstressed
  length 125
endphoneme

phoneme i
  vowel starttype (i) endtype (i)
  length 160
  formants vowel/i_2
  after w w/wi2
endphoneme

phoneme I
  vowel starttype (i) endtype (i)
  length 130
  formants vowel/ii_2
endphoneme

phoneme y
  vowel starttype (u) endtype (u)
  length 180
  formants vowel/y
endphoneme

phoneme 8
  vowel starttype (u) endtype (u)
  length 140
  formants vowel/oe
endphoneme

phoneme u
  vowel starttype (u) endtype (u)
  length 180
  formants vowel/u
endphoneme

phoneme E
  vowel starttype (e) endtype (e)
  length 140
  formants vowel/ee
endphoneme

phoneme E2    //  as[E] but reduces to @ if unstressed
  vowel starttype (e) endtype (e)
  length 140
  formants vowel/ee_2
  reduceto @  0
endphoneme

phoneme W
  vowel starttype (@) endtype (@)
  length 160
  formants vowel/oe
endphoneme

phoneme O
  vowel starttype (o) endtype (o)
  length 150
  formants vowel/oo
endphoneme

phoneme a:
  vowel starttype (a) endtype (a)
  length 210
  formants vowel/a
endphoneme

phoneme A
  vowel starttype (a) endtype (a)
  length 160
  formants vowel/aa_2
endphoneme


// Diphthongs
//===========

phoneme e:
  vowel starttype (e) endtype (i)
  length 190
  formants vdiph/ei
endphoneme

phoneme Y:
  vowel starttype (@) endtype (@)
  length 220
  formants vdiph/y#y
endphoneme

phoneme o:
  vowel starttype (o) endtype (u)
  length 200
  formants vdiph/ou
endphoneme


phoneme EI
  vowel starttype (a) endtype (i)
  length 210
  formants vdiph/&i
endphoneme

phoneme Wy
  vowel starttype (@) endtype (u)
  length 220
  formants vdiph/&y
endphoneme

phoneme VU
  vowel starttype (a) endtype (u)
  length 220
  formants vdiph/Vu
endphoneme

phoneme eU
  vowel starttype (e) endtype (u)
  length 220
  formants vdiph/eu
endphoneme

phoneme yU
  vowel starttype (u) endtype (u)
  length 220
  formants vdiph2/yu
endphoneme




// CONSONANTS
//===========


phoneme r
  liquid
  lengthmod 7
  beforenotvowel r/
  trill
  formants r/rr
  after _ r/rr
  after t r/trr p r/trr k r/trr
  before _ r/r_
endphoneme


phoneme r/   // variant of [r] when not preceding a vowel
  liquid vowel2
  length 100
  lengthmod 7   //
  formants NULL
endphoneme


phoneme v2        // approximant, not fricative
  vcd lbd frc
  vowelin  f1=0  f2=1000 -300 -200  f3=-300 100
  vowelout f1=0  f2=1000 -500 -300  f3=-300 60  len=50
  lengthmod 6
  formants voc/v
  before _ voc/v_
  switchvoicing f
endphoneme


phoneme x2        // a stronger version of [x] (for Afrikaans)
  vls vel frc
  vowelin f1=0  f2=2300 200 400  f3=-100 100
  vowelout f1=0 f2=2300 300 400  f3=-100 100  rms=20
  length 100
  lengthmod 3
  wave ufric/x2      // stronger version
endphoneme


