phoneme l   // syllabic l
  vwl  lng starttype #@  endtype #@
  ipa lU+0329
  unstressed
  length 200
  FMT(vwl_hi/l-voc)
endphoneme

phoneme e
  vwl lng starttype #e endtype #e
  length 220
  FMT(vowel/e)
endphoneme

phoneme o
  vwl lng starttype #o endtype #o
  length 220
  FMT(vowel/o)
endphoneme

phoneme E
  vwl lng starttype #e endtype #e
  length 220
  FMT(vowel/e_mid)
endphoneme

phoneme O
  vwl lng starttype #o endtype #o
  length 220
  FMT(vowel/oo_4)
endphoneme

