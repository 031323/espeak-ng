phoneme a
  vwl lng starttype #a endtype #a
  length 220
  FMT(vowel/a#_3)
endphoneme

phoneme A
  vwl lng starttype #a endtype #a
  length 220
  FMT(vowel/aa_9)
endphoneme

phoneme i
  vwl lng starttype #i endtype #i
  length 220
  FMT(vowel/ii_3)
endphoneme

phoneme u
  vwl lng starttype #u endtype #u
  length 220
  FMT(vowel/u)
endphoneme

