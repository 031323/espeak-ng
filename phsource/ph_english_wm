

//*******************************************************************
// VOWELS
//*******************************************************************


phoneme @    //  Schwa
  vowel   starttype (@)  endtype (@)
  formants vowel/@
  unstressed
  linkout r-
  length 140
endphoneme


phoneme @2   //  Schwa, changes to I before a vowel
  vowel  starttype (@)  endtype (@)
  formants vowel/@
  unstressed
  beforevowel I
  length 120
endphoneme

phoneme @5   //  Schwa, changes to U before a vowel
  vowel  starttype (@)  endtype (@)
  formants vowel/@
  unstressed
  beforevowelpause U
  length 140
endphoneme


phoneme @L
  vowel  starttype (@)  endtype l
  length 160
  formants vwl_en/@L
  unstressed
  before _ vwl_en/@L
endphoneme


phoneme a
  vowel  starttype (a)  endtype (a)
  length 170
  formants vwl_en_wm/a
  reduceto a2  0
endphoneme


phoneme E
  vowel  starttype (e)  endtype (e)
  length 150
  formants vwl_en_rp/E
  reduceto I  0
//  before l/ DFT-30+L/eL_af
endphoneme


phoneme I
  vowel  starttype (i) endtype (i)
  length 140
  formants vwl_en_wm/I
  before l vowel/I
  linkout ;
endphoneme

phoneme i   //  optional variant of [I] for end of words
  vowel  starttype (i)  endtype (i)
  length 220
  formants vowel/i:
  linkout ;
endphoneme


phoneme 0
  vowel  starttype (o)   endtype (o)
  length 140
  formants vwl_en_n/0
  reduceto @  0
endphoneme


phoneme V
  vowel  starttype (o)  endtype (o)
  length 140
  formants vwl_en_n/V
  reduceto @  0
endphoneme


phoneme U
  vowel  starttype (u)  endtype (u)
  length 150
  formants vowel/U
endphoneme


phoneme I2
  vowel  starttype (i)  endtype (i)
  unstressed
  length 140
  formants vwl_en_wm/I
  before l vowel/I
  linkout ;
endphoneme


phoneme a2
  vowel  starttype (@)  endtype (@)
  unstressed
  length 140
  formants vwl_en/a2
//  linkout r-
endphoneme


phoneme A:
  vowel  starttype (a)  endtype (a)
  length 240
  formants vwl_en_wm/A:
  linkout r-
endphoneme


phoneme aa   //   'bath' etc.
  vowel  starttype (a)  endtype (a)
  length 150
  formants vwl_en_wm/a
  linkout r-
endphoneme


phoneme 3:
  vowel  starttype (@)  endtype (@)
  length 250
  formants vowel/Y
  reduceto @  0
  linkout r-
endphoneme


phoneme i:
  vowel  starttype (i)  endtype (i)
  length 220
  formants vwl_en_wm/i:
  after w w/wi:
  linkout ;
endphoneme


phoneme O:
  vowel  starttype (o)  endtype (o)
  length 240
  formants vowel/O:
  reduceto @  0
  linkout r-
endphoneme


phoneme O@
  vowel  starttype (o)  endtype (@)
  length 240
  formants vowel/o:
  linkout r-
endphoneme


phoneme O
  vowel  starttype (o)  endtype (o)
  length 150
  formants vowel/o:
  reduceto @  0
endphoneme


phoneme u:
  vowel  starttype (u)  endtype (u)
  length 200
  formants vwl_en_wm/@u
//  before l/ vwl_en/u:L2-40+L/uL
endphoneme


phoneme aU
  vowel  starttype (a)  endtype (u)
  length 230
  formants vwl_en_n/aU
endphoneme


phoneme oU
  vowel  starttype (@)  endtype (u)
  length 220
  formants vwl_en_wm/oU
endphoneme


phoneme aI
  vowel  starttype (o)  endtype (i)
  length 240
  formants vwl_en_wm/aI
endphoneme


phoneme eI
  vowel  starttype (@)  endtype (i)
  length 210
  formants vwl_en_wm/eI
endphoneme


phoneme OI
  vowel  starttype (o)  endtype (i)
  length 230
  formants vowel/oI
endphoneme


phoneme e@
  vowel  starttype (e)  endtype (@)
  length 240
  formants vwl_en/3:
  linkout r-
endphoneme


phoneme I@
  vowel  starttype (i)  endtype (@)
  length 240
  formants vwl_en/i@
  linkout r-
